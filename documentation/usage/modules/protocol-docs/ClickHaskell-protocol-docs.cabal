Cabal-version:      3.0
name:               ClickHaskell-protocol-docs
version:            0.0.0.1
build-type:         Simple
license:            BSD-3-Clause

library
  default-language: Haskell2010
  Exposed-Modules:
    ChProtocolDocs
  build-depends:
      ClickHaskell
    , base >=4.7 && <5
    , bytestring
    , blaze-html
    , blaze-markup
  ghc-options:
    -Wall
    -Wunused-packages
