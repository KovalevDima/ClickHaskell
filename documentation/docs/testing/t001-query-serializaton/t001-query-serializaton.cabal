Cabal-version:      3.0

name:               t001-query-serializaton
version:            0.0.0.1
build-type:         Simple
license:            BSD-3-Clause

executable t001-query-serializaton
  main-is: index.lhs
  hs-source-dirs: .
  ghc-options:
    -O2 -threaded -rtsopts
    -pgmL markdown-unlit -Wall
  build-tool-depends: markdown-unlit:markdown-unlit
  build-depends:
      ClickHaskell
    , async
    , base >=4.7 && <5
    , bytestring
    , time
  default-language: Haskell2010
