Cabal-version:      3.0
name:               ClickHaskell-protocol-docs
version:            0.0.0.1
build-type:         Simple
license:            BSD-3-Clause

executable gen-protocol-docs
  main-is: ChProtocolDocs.hs
  build-depends:
      ClickHaskell
    , base >=4.7 && <5
    , directory
    , cmark
    , text
  ghc-options:
    -Wall
    -Wunused-packages
    -threaded
  default-language: Haskell2010
